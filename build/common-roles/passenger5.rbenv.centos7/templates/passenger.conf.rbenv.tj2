LoadModule passenger_module /usr/local/rbenv/versions/{{ ruby_version }}/lib/ruby/gems/{{ ruby_library_version }}/gems/passenger-{{ passenger_version }}/buildout/apache2/mod_passenger.so
<IfModule mod_passenger.c>
  PassengerRoot /usr/local/rbenv/versions/{{ ruby_version }}/lib/ruby/gems/{{ ruby_library_version }}/gems/passenger-{{ passenger_version }}
  PassengerDefaultRuby /usr/local/rbenv/versions/{{ ruby_version }}/bin/ruby
  PassengerMaxPoolSize 6
  PassengerMaxInstancesPerApp 0
  PassengerPoolIdleTime 300
</IfModule>
